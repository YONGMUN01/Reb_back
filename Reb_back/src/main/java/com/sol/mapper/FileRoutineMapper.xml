<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fileRoutine">
	<select id="select" parameterType="int" resultType="FileRoutineDTO">
		SELECT * FROM TBL_FILE_routine WHERE routine_NUMBER = #{routineNumber}
	</select>
	
	<insert id="insert" parameterType="FileRoutineDTO">
		<!-- fileSystemName 어떻게 지정해야하는지 -->
		INSERT INTO TBL_FILE_routine(file_system_name, file_original_name, routine_number)
		VALUES(#{fileSystemName}, #{fileOriginalName}, #{routineNumber})
	</insert>
	
	<delete id="delete" parameterType="int">
		<!-- fileSystemName 어떻게 지정해야하는지 -->
		DELETE FROM TBL_FILE_routine
		WHERE routine_number = #{routineNumber}
	</delete>
</mapper>